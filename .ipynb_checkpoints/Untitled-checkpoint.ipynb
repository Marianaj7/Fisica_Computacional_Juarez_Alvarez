    "function metodo_newton(f,df,x)\n",
    "    x=x0\n",
    "    f=f(x0)\n",
    "    df=df(x0)\n",
    "    for i in 1:100\n",
    "        x=x-(f/df)\n",
    "    end\n",
    "    return x\n",
    "end"
   ]
